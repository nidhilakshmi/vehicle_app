<p id="notice"><%= notice %></p>

<h1>Transport Systems</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Model</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transport_systems.each do |transport_system| %>
      <tr>
        <td><%= transport_system.name %></td>
        <td><%= transport_system.model %></td>
        <td><%= link_to 'Show', transport_system %></td>
        <td><%= link_to 'Edit', edit_transport_system_path(transport_system) %></td>
        <td><%= link_to 'Destroy', transport_system, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transport System', new_transport_system_path %>
